#include <iostream>

using namespace std;

int main() {
    int n;
    cin >> n; // 43
    int sum = 0;
    // 100
    sum += n / 100;   // 1
    n = n % 100;              // 43

    // 20
    sum += n / 20;     // 2
    n = n % 20;              // 3

    // 10
    sum += n / 10;     // 0
    n = n % 10;               // 3

    // 5
    sum += n / 5;     // 0
    n = n % 5;              // 3

    // 1
    sum += n;
    
    cout << sum;
    return 0;
}





